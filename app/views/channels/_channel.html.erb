<table>
  <tr>
    <th>Name</th>
    <th>Slug</th>
    <th>Image</th>
  </tr>

  <tr>
    <td><%= channel.name %></td>
    <td><%= channel.slug %></td>
    <td><%= channel.image %></td>
    <td><%= link_to 'Show', source_channel_path(channel.source, channel) %></td>
    <td><%= link_to 'Edit', edit_source_channel_path(channel.source, channel) %></td>
    <td><%= link_to 'Destroy', source_channel_path(channel.source, channel),
                    method: :delete,
                    data: { confirm: 'Are you sure?' } %></td>
  </tr>
</table>
